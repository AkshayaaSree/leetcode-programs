class Solution {
    public int[] twoSum(int[] nums, int target) {
        int a[]=new int[2];
        int l=nums.length;
        for(int i=0;i<l;i++)
        {
         for(int j=0;j<l;j++)
         {
            if(nums[i]+nums[j]==target)
            {
                if(i==j) continue;
                a[0]=j;
                a[1]=i;
                break;
            }
            

            
         }

        }
        return a;
    }
    
}
